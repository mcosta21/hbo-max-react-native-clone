{
  "expo": {
    "name": "HBO Max Clone",
    "slug": "hbo-max-react-native-clone",
    "privacy": "public",
    "description": "This is a clone app by HBO Max using React Native Expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#191919"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#191919"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
